{
  "name": "emoji-fusion",
  "private": true,
  "version": "0.1.0",
  "description": "A project to fuse two emojis into one using Next.js, React and Tailwind CSS.",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "analyze": "cross-env ANALYZE=true next build",
    "docker:build": "docker build -t emoji-fusion .",
    "docker:run": "docker run -p 3000:3000 emoji-fusion",
    "docker:dev": "docker-compose up",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emoji-mart/data": "^1.2.1",
    "@emoji-mart/react": "^1.1.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node-fetch": "^2.6.12",
    "@types/three": "^0.174.0",
    "cors": "^2.8.5",
    "emoji-mart": "^5.6.0",
    "express": "^4.21.2",
    "gsap": "^3.12.7",
    "next": "^14.0.0",
    "node-fetch": "^2.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "prom-client": "^15.1.3",
    "serverless-http": "^3.2.0",
    "three": "^0.174.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.0.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.0.0",
    "nodemon": "^3.1.9",
    "postcss": "^8.4.27",
    "rollup-plugin-visualizer": "^5.14.0",
    "tailwindcss": "^3.3.3",
    "tsx": "^4.19.3",
    "typescript": "^5.0.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "keywords": [
    "emoji",
    "fusion",
    "react",
    "tailwindcss",
    "nextjs",
    "docker"
  ],
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.10.0",
  "author": "Xiaotan",
  "license": "MIT"
}
