{
  "name": "emoji-fusion",
  "private": true,
  "version": "0.0.1",
  "description": "A project to fuse two emojis into one using React and Tailwind CSS.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"nodemon --exec tsx src/server/server.ts\"",
    "build": "tsc && vite build",
    "start": "cross-env NODE_ENV=production tsx src/server/server.ts",
    "start:prod": "cross-env NODE_ENV=production PORT=80 tsx src/server/server.ts",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emoji-mart/data": "^1.2.1",
    "@emoji-mart/react": "^1.1.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node-fetch": "^2.6.12",
    "@types/three": "^0.174.0",
    "cors": "^2.8.5",
    "emoji-mart": "^5.6.0",
    "express": "^4.21.2",
    "gsap": "^3.12.7",
    "node-fetch": "^2.7.0",
    "package": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "serverless-http": "^3.2.0",
    "three": "^0.174.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.14",
    "concurrently": "8.2.2",
    "cross-env": "7.0.5",
    "nodemon": "^3.1.9",
    "postcss": "^8.4.27",
    "rollup-plugin-visualizer": "^5.14.0",
    "serverless-http": "^3.2.0",
    "tailwindcss": "^3.3.3",
    "tsx": "^4.19.3",
    "typescript": "^5.0.2",
    "vite": "^5.0.0"
  },
  "keywords": [
    "emoji",
    "fusion",
    "react",
    "tailwindcss"
  ],
  "author": "Xiaotan",
  "license": "MIT"
}
